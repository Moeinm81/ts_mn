{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6","dom"
    ],
    "module": "commonjs",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "*": [
        "node_modules/*"
      ]
    }
  },
  "extends": "../../tsconfig.json",
  "exclude": ["node_modules/**/*"]
  
}